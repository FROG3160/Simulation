    "version": "0.2.0",
    "configurations": [



        {
            "name": "Robot Sim",
            "type": "debugpy",
            "request": "launch",
            "module": "robotpy",
            "cwd": "${workspaceFolder}/roborio",
            "args": ["sim"],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Robot Test",
            "type": "debugpy",
            "request": "launch",
            "module": "robotpy",
            "cwd": "${workspaceFolder}/roborio",
            "args": ["test"],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
    ]
}